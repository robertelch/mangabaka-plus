{
  "manifest_version": 3,
  "name": "JSON Search Injector",
  "version": "1.0",
  "description": "Injects content and searches JSON",
  "host_permissions": [
    "https://mangabaka.dev/*"
  ],
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://mangabaka.dev/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.json"
      ],
      "matches": [
        "https://mangabaka.dev/*"
      ]
    }
  ]
}